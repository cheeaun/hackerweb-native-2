{
  "cli": {
    "requireCommit": false,
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "${EXPO_APPLE_ID}",
        "appleTeamId": "${EXPO_APPLE_TEAM_ID}",
        "ascAppId": "${EXPO_ASC_APP_ID}"
      }
    },
    "preview": {
      "ios": {
        "appleId": "${EXPO_APPLE_ID}",
        "appleTeamId": "${EXPO_APPLE_TEAM_ID}",
        "ascAppId": "${EXPO_ASC_APP_ID}"
      }
    }
  }
}
