{
  "name": "hackerweb",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npx --yes expo start",
    "android": "npx --yes expo start --android",
    "ios": "npx --yes expo start --ios",
    "web": "npx --yes expo start --web",
    "eject": "npx --yes expo eject",
    "current-version": "jq '.expo.version' app.json -r",
    "current-expo-version": "jq '.dependencies.expo' package.json -r | grep -o -E '[0-9]{2}'",
    "current-v": "echo \"$(npm run current-version --silent)-$(npm run current-expo-version --silent)\"",
    "last-eas-build": "eas build:list --limit=1 --buildProfile=preview --json --non-interactive | jq -r .[0].appBuildVersion",
    "current-prod-release": "jq '.build.production.ios.releaseChannel' eas.json -r",
    "last-prod-release": "eas build:list --limit=1 --buildProfile=production --json --non-interactive | jq -r .[0].releaseChannel",
    "current-eas-build": "jq -r '.expo.ios.buildNumber' app.json",
    "envsubst-eas-json": "envsubst < eas.json > eas.tmp && mv eas.tmp eas.json",
    "preeas-submit-build": "set -a && source .env && set +a && npm run envsubst-eas-json",
    "eas-submit-build": "eas build -p ios --profile=preview",
    "posteas-submit-build": "git checkout eas.json",
    "preeas-build-submit-ci": "npm run envsubst-eas-json",
    "eas-build-submit-ci": "eas build -p ios --profile=preview --non-interactive --auto-submit",
    "eas-build-sim": "eas build -p ios --profile=simulator",
    "build": "npx --yes expo build:ios --release-channel staging-`npm run current-version --silent`",
    "eas-build-prod": "eas build -p ios",
    "preeas-build-submit-ci-prod": "npm run envsubst-eas-json",
    "eas-build-submit-ci-prod": "eas build -p ios --non-interactive --auto-submit",
    "build-prod": "npx --yes expo build:ios --release-channel production-`npm run current-version --silent`",
    "latest-staging": "npx --yes expo publish:history -p ios --count 1 -r -c staging-`npm run current-v --silent`",
    "latest-publish-id": "npm run latest-staging --silent | jq '.queryResult[0].publicationId' -r",
    "publish": "npx --yes expo publish --release-channel staging-`npm run current-v --silent`",
    "promote-prod": "npx --yes expo publish:set --publish-id `npm run latest-publish-id --silent` --release-channel production-`npm run current-version --silent`",
    "viz": "npx --yes react-native-bundle-visualizer",
    "deploy-staging": "if [ $(npm run last-prod-release --silent) = $(npm run current-prod-release --silent) ]; then if [ $(npm run last-eas-build --silent) = $(npm run current-eas-build --silent) ]; then npm run publish; else npm run eas-build-submit-ci; fi; fi",
    "deploy-production": "npm run eas-build-submit-ci-prod",
    "expo-version": "npx --yes expo --version"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/hooks": "~2.8.1",
    "@react-native-masked-view/masked-view": "0.2.8",
    "@react-native-segmented-control/segmented-control": "2.4.0",
    "@react-navigation/native": "~6.1.1",
    "@react-navigation/native-stack": "~6.9.7",
    "@react-navigation/stack": "~6.3.10",
    "@sindresorhus/safe-stringify": "~0.1.1",
    "array-move": "~4.0.0",
    "buffer": "~6.0.3",
    "date-fns": "~2.29.3",
    "entities": "~4.4.0",
    "expiry-map": "~2.0.0",
    "expo": "^47.0.9",
    "expo-blur": "~12.0.1",
    "expo-clipboard": "~4.0.1",
    "expo-constants": "~14.0.2",
    "expo-device": "~5.0.0",
    "expo-haptics": "~12.0.1",
    "expo-linear-gradient": "~12.0.1",
    "expo-linking": "~3.3.0",
    "expo-mail-composer": "~12.0.0",
    "expo-status-bar": "~1.4.2",
    "expo-store-review": "^6.0.0",
    "expo-updates": "~0.15.6",
    "expo-web-browser": "~12.0.0",
    "htmlparser2": "~8.0.1",
    "ky": "~0.31.4",
    "p-debounce": "~4.0.0",
    "p-memoize": "^7.1.1",
    "parse5": "~7.1.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-modalize": "~2.1.1",
    "react-native-reanimated": "~2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.2",
    "react-native-svg": "13.4.0",
    "react-native-url-polyfill": "~1.3.0",
    "react-native-view-shot": "3.4.0",
    "react-native-web": "0.18.10",
    "react-native-webview": "11.23.1",
    "react-syntax-highlighter": "~15.5.0",
    "stream": "~0.0.2",
    "strip-indent": "~4.0.0",
    "url-regex-safe": "~3.0.0",
    "zustand": "~4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@trivago/prettier-plugin-sort-imports": "~4.0.0",
    "metro-minify-terser": "~0.73.6",
    "react-native-svg-transformer": "~1.0.0"
  }
}
